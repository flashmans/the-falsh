<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>计算器</title>
	</head>
	<body>
		<table border="5px" bgcolor="grey" cellspacing="0" align="center" width="400px">
			<tr>
				<td height="40">
					&nbsp;&nbsp;
					<font size="5">•</font>
				</td>
			</tr>
			<tr>
				<td height="40" bgcolor="white">
					
				</td>
			</tr>
		</table>
		<table bgcolor="grey" border="" align="center" width="400px" bordercolor="white">
			<tr height="50" align="center">
				<td width="100px">AC</td>
				<td width="100px">+/-</td>
				<td width="100px">%</td>
				<td width="100px">/</td>	
			</tr>
			
			<tr height="50" align="center">
				<td width="100px">7</td>
				<td width="100px">8</td>
				<td width="100px">9</td>
				<td width="100px">*</td>	
			</tr>
			
			<tr height="50" align="center">
				<td width="100px">4</td>
				<td width="100px">5</td>
				<td width="100px">6</td>
				<td width="100px">-</td>	
			</tr>
			
			<tr height="50" align="center">
				<td width="100px">1</td>
				<td width="100px">2</td>
				<td width="100px">3</td>
				<td width="100px">+</td>	
			</tr>
			
			<tr height="50" align="center">
				<td width="100px">0</td>
				<td width="100px">保留</td>
				<td width="100px">.</td>
				<td width="100px">=</td>	
			</tr>
		</table>
	</body>
</html>
